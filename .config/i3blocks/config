separator_block_width=15

[volume]
command=$HOME/.config/i3blocks/volume
label=VOL 
interval=once
signal=10
STEP=5%

[disk]
command=$HOME/.config/i3blocks/disk
label=HOME 
interval=30

[vpn_status]
command=$HOME/.config/i3blocks/vpn
interval=5
color=#FFFFFF
markup=pango

[bandwidth]
command=$HOME/.config/i3blocks/bandwidth
interval=persist
markup=pango
LABEL=""
UNIT=MB
PRINTF_COMMAND=printf "%d/%d %s/s\n", rx, wx, unit;

[memory]
command=$HOME/.config/i3blocks/memory
label=MEM 
interval=10

[cpu_usage]
command=$HOME/.config/i3blocks/cpu_usage
label=CPU 
interval=10

[time]
command=date '+%Y/%m/%d %H:%M:%S'
interval=1
