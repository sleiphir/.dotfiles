separator_block_width=30

[battery]
command=$HOME/.config/i3blocks/battery
interval=30
label=BAT

[volume]
command=$HOME/.config/i3blocks/volume
label=VOL
interval=once
signal=10

[disk]
command=$HOME/.config/i3blocks/disk
label=HOME
interval=30

[vpn]
command=$HOME/.config/i3blocks/vpn
interval=5
color=#FFFFFF
markup=pango

[bandwidth]
command=$HOME/.config/i3blocks/bandwidth
interval=persist
markup=pango
label=

[memory]
command=$HOME/.config/i3blocks/memory
label=MEM
interval=10

[cpu_usage]
command=$HOME/.config/i3blocks/cpu_usage
label=CPU
interval=10

[time]
command=date '+%Y/%m/%d %H:%M:%S'
interval=1
